{
  "name": "reloop-keypad",
  "version": "0.0.0",
  "description": "Reloop KeyPad - MIDI Configuration Editor",
  "bin": {
    "keypad": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dubnix/reloop-keypad.git"
  },
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dubnix/reloop-keypad/issues"
  },
  "homepage": "https://github.com/dubnix/reloop-keypad",
  "scripts": {
    "lint": "eslint bin lib"
  },
  "tarima": {
    "cacheFile": "dist/index.json",
    "public": "dist/public",
    "dest": "dist",
    "bundle": "**/*.js",
    "rename": [
      "**/src/**:{filepath/1}/{filename}.{extname}"
    ],
    "filter": [
      "!_*",
      "!**/_*",
      "!**/_*/**"
    ],
    "bundleOptions": {
      "extensions": {
        "js": "es6"
      }
    }
  },
  "devDependencies": {
    "buble": "^0.15.2",
    "chokidar": "^1.6.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.14.0",
    "rollup": "^0.41.4",
    "tarima-cli": "gextech/tarima-cli"
  },
  "dependencies": {
    "express": "^4.11.2",
    "jazz-midi": "^1.4.1",
    "minimist": "^1.1.0",
    "serve-static": "^1.8.1",
    "ws": "^0.7.1"
  }
}
