{
  "name": "reloop-keypad",
  "version": "0.0.0",
  "description": "Reloop KeyPad - MIDI Configuration Editor",
  "bin": {
    "keypad": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dubnix/reloop-keypad.git"
  },
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dubnix/reloop-keypad/issues"
  },
  "homepage": "https://github.com/dubnix/reloop-keypad",
  "scripts": {
    "lint": "eslint bin lib",
    "build": "tarima -fq",
    "dev": "tarima watch"
  },
  "tarima": {
    "public": "dist/public",
    "from": "src",
    "dest": "dist",
    "bundle": true,
    "rename": [
      "**/src/**:{fullpath/2}.js"
    ],
    "filter": [
      "!_*",
      "!**/_*",
      "!**/_*/**",
      "!**/lib/**"
    ],
    "bundleOptions": {
      "extensions": {
        "js": "es6"
      }
    }
  },
  "devDependencies": {
    "buble": "^0.19.6",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "rollup": "^0.67.0",
    "tarima": "^4.2.3"
  },
  "dependencies": {
    "express": "^4.11.2",
    "jazz-midi": "^1.4.1",
    "serve-static": "^1.8.1",
    "wargs": "^0.8.1",
    "ws": "^6.1.0"
  }
}
