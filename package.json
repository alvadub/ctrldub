{
  "name": "reloop-keypad",
  "version": "0.0.0",
  "description": "Reloop KeyPad - MIDI Configuration Editor",
  "bin": {
    "keypad": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dubnix/reloop-keypad.git"
  },
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dubnix/reloop-keypad/issues"
  },
  "homepage": "https://github.com/dubnix/reloop-keypad",
  "scripts": {
    "lint": "eslint bin lib",
    "build": "tarima -fq",
    "dev": "tarima watch"
  },
  "tarima": {
    "public": "www",
    "serve": [
      "build"
    ],
    "from": [
      "src/next",
      "src/webapp"
    ],
    "bundle": true,
    "rename": [
      "**:{fullpath/2}",
      "main.js:KeyPad.control.js"
    ],
    "filter": [
      "!_*",
      "!**/_*",
      "!**/_*/**",
      "!**/{lib,helpers,components}/**"
    ],
    "devPlugins": [
      "tarima-lr"
    ],
    "bundleOptions": {
      "rollup": {
        "plugins": [
          "rollup-plugin-node-resolve",
          "rollup-plugin-commonjs",
          "rollup-plugin-svelte"
        ]
      },
      "extensions": {
        "js": "es6"
      }
    }
  },
  "devDependencies": {
    "buble": "^0.19.7",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "less": "^3.8.1",
    "rollup": "^1.15.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.4",
    "rollup-plugin-svelte": "^5.1.0",
    "svelte": "^3.5.3",
    "tarima": "^4.5.3",
    "tarima-lr": "^0.5.3"
  },
  "dependencies": {
    "express": "^4.11.2",
    "jazz-midi": "^1.4.1",
    "serve-static": "^1.8.1",
    "wargs": "^0.8.2",
    "ws": "^7.0.1"
  }
}
